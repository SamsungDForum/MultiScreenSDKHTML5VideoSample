<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets"
        xmlns:tizen="http://tizen.org/ns/widgets"
        id="http://yourdomain/MultiScreenHTML5Video"
        version="1.1.0" viewmodes="maximized">
    <access origin="*" subdomains="true"></access>
    <tizen:application id="demoapp014.MultiScreenHTML5Video"
                       package="demoapp014"
                       required_version="2.3"/>
    <content src="index.html"/>
    <feature name="http://tizen.org/feature/screen.size.all"/>
    <icon src="icon.png"/>
    <name>SmartViewSDKHTML5VideoSample</name>
    <tizen:privilege name="http://tizen.org/privilege/tv.inputdevice"/>
    <tizen:profile name="tv"/>
    <tizen:setting screen-orientation="auto-rotation"
                   background-support="disable"
                   encryption="disable"
                   hwkey-event="disable"/>
</widget>
